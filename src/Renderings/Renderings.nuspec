<?xml version="1.0"?>
<package>
  <metadata>
    <id>Renderings</id>
    <version>$version$</version>
    <!-- overridden by "NuGet pack -Version" flag -->
    <authors>Brad McDavid</authors>
    <owners>Brad McDavid</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>.NET model rendering framework.</description>
    <releaseNotes>Updated DotNetStarter dependency.</releaseNotes>
    <copyright>Copyright $year$</copyright>
    <tags>rendering viewmodels</tags>
    <dependencies>
      <group targetFramework="net40">
        <dependency id="DotNetStarter.Abstractions" version="[2,)" />
        <dependency id="DotNetStarter.RegistrationAbstractions" version="[1.1.0,)" />
      </group>
      <group targetFramework="net45">
        <dependency id="DotNetStarter.Abstractions" version="[2,)" />
        <dependency id="DotNetStarter.RegistrationAbstractions" version="[1.1.0,)" />
      </group>
      <group targetFramework="netstandard1.0">
        <dependency id="DotNetStarter.Abstractions" version="[2,)" />
        <dependency id="DotNetStarter.RegistrationAbstractions" version="[1.1.0,)" />
        <dependency id="NETStandard.Library" version="1.6.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="**\*.cs" target="src" exclude="obj\**\*.cs" />
    <file src="readme.txt" target="readme.txt" />
    
    <file src="bin\Release\net40\Renderings.dll" target="lib\net40" />
    <file src="bin\Release\net40\Renderings.pdb" target="lib\net40" />
    <file src="bin\Release\net40\Renderings.xml" target="lib\net40" />

    <file src="bin\Release\net45\Renderings.dll" target="lib\net45" />
    <file src="bin\Release\net45\Renderings.pdb" target="lib\net45" />
    <file src="bin\Release\net45\Renderings.xml" target="lib\net45" />

    <file src="bin\Release\netstandard1.0\Renderings.dll" target="lib\netstandard1.0" />
    <file src="bin\Release\netstandard1.0\Renderings.pdb" target="lib\netstandard1.0" />
    <file src="bin\Release\netstandard1.0\Renderings.xml" target="lib\netstandard1.0" />
    
  </files>
</package>